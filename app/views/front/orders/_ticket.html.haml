%table{:border => "0", :cellpadding => "0", :cellspacing => "0", :style => "border-collapse: collapse; border-spacing: 0; margin-top: 0px !important; padding-top: 0px !important; margin-bottom: 10px !important; border: 0;", :width => "592"}
  %tbody
    %tr{:style => "border-collapse: collapse; border-spacing: 0; border: 0;"}
      %td{:align => "left", :height => "160", :valign => "top", :width => "592"}
        %table{:border => "0", :margin => "10", :padding => "0", :style => "vertical-align: top; margin-top: 0px !important; padding-top: 0px !important; border: 1px solid #ccc;", :width => "590"}
          %tr
            %td{:align => "left", :height => "160", :style => "vertical-align: top;", :valign => "top", :width => "450"}
              %div{:style => "margin: 10px;"}
                =ticket.event_starts_at.to_formatted_s(:jammin_java)
                %h1{:style => "font-weight: normal; letter-spacing: normal; word-spacing: 0px; text-shadow: none; color: #000000; font-size: 23px; line-height: 28px; font-family: Arial, 'Helvetica Neue', Helvetica, sans-serif;  margin-top:0; padding-top:0; margin-right:0; padding-right:0; margin-bottom:0; padding-bottom:0; margin-left:0; padding-left:0; "}
                  =ticket.event_name_1
                =ticket.event_name_2
                %br/
                %br/
                %h1{:style => "font-weight: normal; letter-spacing: normal; word-spacing: 0px; text-shadow: none; color: #000000; font-size: 23px; line-height: 28px; font-family: Arial, 'Helvetica Neue', Helvetica, sans-serif; margin: 10px 0 0; padding: 0;"}
                  Section:
                  %b=ticket.section_label
                  -if ticket.table_seat
                    Table:
                    %b= ticket.table_seat[0]
                    Seat:
                    %b= ticket.table_seat[1]
                %span.wysiwyg-color-small{:style => "color: #000000; font-size: 10px; font-family: inherit;"}
                  Jammin Java 227 Maple Ave E, Vienna, VA, 
                  %a{:href => 'http://jamminjava.com', :style => 'color: #990000; '}jamminjava.com, 
                  %a{:href => 'mailto:tickets@jamminjava.com', :style => 'color: #990000; '}tickets@jamminjava.com
            %td{:align => "left", :height => "160", :style => "vertical-align: top;", :valign => "bottom", :width => "142"}
              =image_tag( qr( "https://jamminjava.com/tickets/#{ticket.id}/checkin"), :width => 150)